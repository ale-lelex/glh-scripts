function parseColors(colorString) {
    let parts = colorString.split(' ');

    let angle = parseInt(parts[2]);
    let textColor = parseInt(parts[3], 16);
    let colors = parts.slice(4).map(color => parseInt(color, 16));

    return {
        angle: angle,
        textColor: textColor,
        colors: colors
    };
}

